\n      },\n      items: [],\n      errors: [],\n      empty: [],\n      content1:null,\n      content2:null,\n      dialog: false,\n      sub:null,\n      sub2:null,\n      headers: [\n        {\n          text: 'Título',\n          value: 'titulo'\n        },\n        {\n          text: 'Categoría',\n          value: 'categoria'\n        },\n        {\n          text: 'Fecha',\n          value: 'fecha'\n        },\n        {\n          text: 'Status',\n          value: 'status'\n        },\n        {\n          sortable: false,\n          text: 'Opciones',\n          value: 'opciones',\n          align: 'right'\n        }\n      ]\n    }\n},\n methods: {\n },\ncreated() {\n    //alert(sessionStorage.getItem(\"dato\"))\n    this.info = this.$route.params.info\n    //alert(\"ejemplo \"+this.info)\n    //sessionStorage.setItem(\"content1\",\"display:none\")\n    //sessionStorage.setItem(\"content2\",null)\n    //alert(this.content2)\n    if(sessionStorage.getItem(\"dato\")!=null){\n      this.escrito=sessionStorage.getItem(\"dato\")\n    }else{\n      this.escrito=''\n    }\n    api.get(`/noticias/all`)\n    .then(response => {\n      // JSON responses are automatically parsed.\n      this.items = response.data\n      //console.log(this.items);\n    })\n    .catch(e => {\n      this.errors.push(e)\n    //  console.log(\"Error\");\n    //  console.log(e);\n    })\n}\n\n\n}\n\n</script>\n"]}]}