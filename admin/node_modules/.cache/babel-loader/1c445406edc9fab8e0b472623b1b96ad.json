nia\"\n              :rules=\"rules6\"\n            />\n            <h4> Localidad: </h4>\n            <v-text-field\n              label=\"Localidad\"\n              class=\"purple-input\"\n              name=\"localidad\"\n              :value=\"localidad\"\n              :rules=\"rules9\"\n            />\n            <h4> Municipio: </h4>\n            <v-text-field\n              label=\"Municipio\"\n              class=\"purple-input\"\n              name=\"municipio\"\n              :value=\"municipio\"\n              :rules=\"rules9\"\n              />\n            <h4> Estado: </h4>\n            <v-select\n              label=\"Estado\"\n              class=\"purple-input\"\n              name=\"estado\"\n              :value=\"estado\"\n              :rules=\"rules9\"\n              outlined\n              :items=\"items1\"\n              />\n            <h4> Pais: </h4>\n            <v-text-field\n              label=\"País\"\n              class=\"purple-input\"\n              name=\"pais\"\n              :value=\"pais\"\n              :rules=\"rules9\"\n              />\n            <h4> Código Postal: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Código Postal\"\n              name=\"codigo_postal\"\n              :value=\"codigo_postal\"\n              :rules=\"rules10\"\n              />\n            <h4> Referencias: </h4>\n            <template />\n              <v-textarea\n                class=\"purple-input\"\n                label=\"Referencias\"\n                name=\"referencias\"\n                :rules=\"rules6\"\n                :value=\"referencias\"\n              />\n              <!--\n            <h4> Status: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Status\"\n              name=\"status\"\n              disabled=\"true\"\n              :value=\"status\"\n            />\n            -->\n          </v-flex>\n        </v-layout>\n       