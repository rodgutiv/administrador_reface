o\"\n              name=\"codigo\"\n              disabled=\"true\"\n              :value=\"codigo\"\n            />\n            <h4> Nombre: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Nombre\"\n              name=\"nombre\"\n              :value=\"nombre\"\n            />\n            <h4> Descripción: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Descripción\"\n              name=\"descripcion\"\n              :value=\"descripcion\"\n            />\n            <h4> Categoría: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Categoría\"\n              name=\"categoria\"\n              :value=\"categoria\"\n            />\n            <h4> Subcategoría: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Subcategoría\"\n              name=\"tipo\"\n              :value=\"tipo\"\n            />\n            <h4> Marca: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Marca\"\n              name=\"marca\"\n              :value=\"marca\"\n            />\n            <h4> Autos: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Autos\"\n              name=\"autos\"\n              :value=\"autos\"\n            />\n            <h4> Precio: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Precio\"\n              name=\"precio\"\n              :value=\"precio\"\n              type=\"number\"\n            />\n            <h4> Stock: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Stock\"\n              name=\"stock\"\n              :value=\"stock\"\"\n              type=\"number\"\n            />\n\n            <h4> Promoción: </h4>\n            <v-select\n              :items=\"items_promos\"\n              label=\"Promocion\"\n              :value=\"id_promo\"\n              name=\"id_promo\"\n              outlined\n            ></v-select>\n<!--\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Id_promo\"\n              name=\"id_promo\"\n              :value=\"id_promo\"\n            />\n-->\n            <h4> Servicio Pesado: </h4>\n            <v-select\n              :items=\"items\"\n              label=\"Pesado\"\n              :value=\"pesado\"\n              name=\"pesado\"\n              outlined\n            ></v-select>\n<!--\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Pesado\"\n              name=\"pesado\"\n              :value=\"pesado\"\n            />\n-->\n            <h4> Outlet: </h4>\n            <v-select\n              :items=\"items\"\n              label=\"Outlet\"\n              :value=\"outlet\"\n              name=\"outlet\"\n              outlined\n            ></v-select>\n<!--\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Outlet\"\n              name=\"outlet\"\n              :value=\"outlet\"\n            />\n\n            <h4> Status: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Status\"\n              name=\"status\"\n              :value=\"status\"\n            />\n-->\n          </v-flex>\n        </v-layout>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n\n          <v-btn\n            color=\"green darken-1\"\n            flat=\"flat\"\n            @click=\"dialog = false\"\n          >\n            Cancelar\n          </v-btn>\n\n          <v-btn\n            color=\"green darken-1\"\n            flat=\"flat\"\n            type=\"submit\"\n            @click=\"dialog = false\"\n          >\n            Guardar Cambios\n          </v-btn>\n        </v-card-actions>\n      </v-form>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nimport {api} from '@/api'\nimport $ from 'jquery'\n\nexport default {\n    name: 'Frame',\n    props: {\n      nombre: String,\n      codigo: String,\n      clave_interna: String,\n      descripcion: String,\n      categoria: String,\n      tipo: String,\n      marca: String,\n      precio: Number,\n      autos: String,\n      stock: Number,\n      status: String,\n      autos: String,\n      id_promo: String,\n      pesado: String,\n      outlet: String\n    },\n    data () {\n      return {\n        cant: 0,\n        info:null,\n        dialog: false,\n        items: ['Si','No'],\n        items_promos: [\"0\"],\n        rules1: [\n          value => !!value || 'Required.',\n          value => (value || '').length <= 50 || 'Max 50 characters',\n          value => {\n            const pattern = /^([A-ZÁÉÍÓÚ]{1}[a-zñáéíóú]+[\\s]*)+$/;\n            return pattern.test(value) || 'Nombre(s) invalido(s).'\n          },\n        ],\n        rules2: [\n          value => !!value || 'Required.',\n          value => (value || '').length <= 50 || 'Max 50 characters',\n          value => {\n            const pattern = /^([A-ZÁÉÍÓÚ]{1}[a-zñáéíóú]+[\\s]*)+$/;\n            return pattern.test(value) || 'Apellido(s) invalido(s).'\n          },\n        ]\n      }\n    },\n    methods: {\n       Update(){\n         api.post('/ad-productos/update-product', $(event.currentTarget).serializeArray())\n         .then(response => {\n           // JSON responses are automatically parsed.\n//           this.models = response.data;\n             alert('Actualización realizada con éxito');\n             window.location.replace(\"http://refacenet.org:61/productos\");\n           //console.log(this.items);\n         })\n         .catch(e => {\n//           this.errors.push(e)\n         //  console.log(\"Error\");\n           console.log(e);\n         })\n       }\n    },\n    created() {\n        this.info = this.$route.params.info\n        if(sessionStorage.getItem(\"dato\")!=null){\n          this.escrito=sessionStorage.getItem(\"dato\")\n        }else{\n          this.escrito=''\n        }\n        api.get(`/promociones/all`)\n        .then(response => {\n          // JSON responses are automatically parsed.\n          for(var i=0; i<response.data.length; i++)\n            this.items_promos.push(response.data[i].id_promo);\n\n\n//          this.items_promos = response.data\n          console.log(this.items_promos);\n        })\n        .catch(e => {\n          this.errors.push(e)\n        //  console.log(\"Error\");\n        //  console.log(e);\n        })\n    }\n}\n</script>\n"],"sourceRoot":"src/components/core"}]}