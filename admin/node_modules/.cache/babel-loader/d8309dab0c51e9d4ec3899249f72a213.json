4> Título: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Título\"\n              name=\"titulo\"\n              id=\"titulo\"\n              :rules=\"rules\"\n            />\n            <h4> Categoría: </h4>\n            <v-text-field\n              class=\"purple-input\"\n              label=\"Categoría\"\n              name=\"categoria\"\n              id=\"categoria\"\n              :rules=\"rules\"\n            />\n            <h4> Contenido: </h4>\n            <v-textarea\n              outline\n              class=\"purple-input\"\n              label=\"Contenido\"\n              name=\"contenido\"\n              id=\"contenido\"\n              spellcheck=\"true\"\n              rows=\"15\"\n              :rules=\"rules\"\n            />\n            <h4> Imágen Asociada: </h4>\n            <input\n              label=\"Imagen: \"\n              name=\"img1\"\n              id=\"img1\"\n              accept=\"image/*\"\n              type=\"file\"\n              :rules=\"rules\"\n            />\n          </v-flex>\n        </v-layout>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n\n          <v-btn\n            color=\"green darken-1\"\n            flat=\"flat\"\n            @click=\"dialog = false\"\n          >\n            Cancelar\n          </v-btn>\n\n          <v-btn\n            color=\"green darken-1\"\n            flat=\"flat\"\n            type=\"submit\"\n            @click=\"dialog = false\"\n          >\n            Guardar Cambios\n          </v-btn>\n        </v-card-actions>\n      </v-form>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nimport {api} from '@/api'\nimport $ from 'jquery'\n\nexport default {\n    name: 'Frame',\n    props: {\n      ruta: String,\n      titulo: String,\n      categoria: String,\n      contenido: String,\n      descripcion: String,\n      status: String\n    },\n    data () {\n      return {\n        cant: 0,\n        info:null,\n        dialog: false,\n        mess: null,\n        rules: [\n          value => !!value || 'Required.'\n        ]\n      }\n    },\n    methods: {\n       Update(){\n\n            var form = $('#form')[0];\n            var data = new FormData(form);\n\n            var check1 = document.getElementById(\"titulo\").value;\n            var check2 = document.getElementById(\"categoria\").value;\n            var check3 = document.getElementById(\"contenido\").value;\n            var check4 = document.getElementById(\"img1\").value;\n\n            console.log(check1);\n            console.log(check2);\n            console.log(check3);\n            console.log(check4);\n\n            if( check1==\"\" || check2==\"\" || check3==\"\" || check4==\"\")\n            {\n              alert(\"Necesita completar todos los campos\");\n            }\n            else\n            {\n            $.ajax({\n                type: \"POST\",\n                enctype: 'multipart/form-data',\n                url: \"http://refacenet.org:61/noticias/new-noticia\",\n                data: data,\n                processData: false,\n                contentType: false,\n                cache: false,\n                timeout: 600000,\n                success: function (data) {\n                    console.log(\"SUCCESS : \", data);\n                  alert(\"Noticia añadida exitosamente\");\n                  window.location.replace(\"http://refacenet.org:61/noticias\");\n                },\n                error: function (e) {\n                    console.log(\"ERROR : \", e);\n                }\n            });\n            }\n       }\n    }\n}\n</script>\n"],"sourceRoot":"src/components/core"}]}